{
  "name": "full-command",
  "version": "1.0.0",
  "description": "Lista intuitiva de comandos equivalentes dos principais sistemas operacionais",
  "main": "index.js",
  "scripts": {
    "pre-build": "rm -rf dist/ && mkdir -p dist/data && cp -R data dist",
    "watch": "parcel --open --no-autoinstall ./src/index.html",
    "start": "npm run pre-build && npm run watch",
    "build": "npm run pre-build && parcel build ./src/index.html --public-url ./",
    "lint": "tslint \"./src/**/*.{ts,tsx,js,jsx}\"",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eletrodos/full-command.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Eletrodos/full-command/issues"
  },
  "homepage": "https://github.com/Eletrodos/full-command#readme",
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "parcel": "^1.10.3",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "prettier": "^1.16.3",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "tslint-react-a11y": "^1.0.0",
    "typescript": "^3.1.6"
  }
}
